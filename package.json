{
  "name": "calistenics-workout",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run generate-icons && tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test:storybook": "test-storybook",
    "generate-icons": "node scripts/generate-icons.js",
    "postinstall": "echo 'Dependency installation complete with legacy-peer-deps enabled'",
    "check": "tsc --noEmit && eslint .",
    "ci": "CI=true && npm run lint && tsc --noEmit && npm run test"
  },
  "dependencies": {
    "@radix-ui/themes": "^3.2.1",
    "@vitejs/plugin-react-swc": "^3.9.0",
    "radix-ui": "^1.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^6.30.0",
    "react-router-dom": "^6.30.0",
    "swr": "2.3.3",
    "usehooks-ts": "3.1.1",
    "vite-plugin-pwa": "^1.0.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.6",
    "@eslint/js": "^9.21.0",
    "@storybook/addon-essentials": "^8.6.12",
    "@storybook/addon-onboarding": "^8.6.12",
    "@storybook/blocks": "^8.6.12",
    "@storybook/experimental-addon-test": "^8.6.12",
    "@storybook/react": "^8.6.12",
    "@storybook/react-vite": "^8.6.12",
    "@storybook/test": "^8.6.12",
    "@storybook/test-runner": "0.22.0",
    "@testing-library/react": "^16.3.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/browser": "^3.1.1",
    "@vitest/coverage-v8": "^3.1.1",
    "concurrently": "8.2.2",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-storybook": "^0.12.0",
    "globals": "^15.15.0",
    "http-server": "14.1.1",
    "jsdom": "^26.1.0",
    "playwright": "^1.52.0",
    "prettier": "^3.5.3",
    "sharp": "^0.34.1",
    "storybook": "^8.6.12",
    "storybook-addon-react-router-v6": "^2.0.15",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0",
    "vitest": "^3.1.1",
    "wait-on": "7.2.0"
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "config": {
    "legacy-peer-deps": true
  }
}
